{
  "name": "site-analise-de-dados-completa",
  "version": "2.0.0",
  "description": "Plataforma completa de análise de dados - A ferramenta mais avançada para ciência de dados, estatística e machine learning",
  "main": "index.js",
  "scripts": {
    "install-frontend": "cd frontend && npm install",
    "install-backend": "cd backend && pip install -r requirements.txt",
    "build": "cd frontend && npm run build",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:frontend": "cd frontend && npm run dev",
    "start:backend": "cd backend && python main.py",
    "deploy": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cordeirotelecom/site-analise-de-dados-completa.git"
  },
  "keywords": [
    "data-science",
    "analytics",
    "statistics",
    "machine-learning",
    "dashboard",
    "visualization",
    "python",
    "react",
    "typescript",
    "fastapi",
    "scientific-computing",
    "dados-publicos",
    "santa-catarina",
    "ibge",
    "spss-alternative",
    "sas-alternative",
    "stata-alternative"
  ],
  "author": "cordeirotelecom",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cordeirotelecom/site-analise-de-dados-completa/issues"
  },
  "homepage": "https://github.com/cordeirotelecom/site-analise-de-dados-completa#readme",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
